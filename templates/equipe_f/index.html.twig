{% extends 'base-front.html.twig' %}
{% block title %}Equipe {% endblock %}

{% block body %}


    
      <div class="blog-section spad mx-auto">
        
        {% for equipe in equipes1 %}
            <tr>
                <td> {{ equipe.idequipe }}</td>
                <td>{{ equipe.nom }}</td>
                <td>{{ equipe.logo }}</td>
                 <td><img src="{{ asset('images/' ~ equipe.logo) }}"
                alt="" width="100" height="100">
                </td>
                <td> {{ equipe.score }}</td>
                <td>{{ equipe.region }}</td>
                <td>
                    <a href="{{ path('app_equipe_show', {'idequipe': equipe.idequipe}) }}">show</a>
                  
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
    
</div>

{% endblock %}

